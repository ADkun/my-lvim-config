{
    "name": "adkun-snippets",
    "author": "adkun",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "plaintext",
                    "markdown",
                    "global",
                    "all"
                ],
                "path": "./global.json"
            },
            {
                "language": "json",
                "path": "./snippets/json.json"
            },
            {
                "language": "python",
                "path": "./snippets/python.json"
            },
            {
                "language": "c",
                "path": "./snippets/c.json"
            },
            {
                "language": "cpp",
                "path": "./snippets/cpp.json"
            }
        ]
    }
}
